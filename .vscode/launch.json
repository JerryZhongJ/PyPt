    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "PyPt",
            "type": "python",
            "request": "launch",
            "module": "PyPt",
            "justMyCode": true,
            "args": ["~/Documents/test_projects/flask/tests", "-a", "-o", "./callgraph.json", "--include", "flask"]

        },
        {
            "name": "Test PTA",
            "type": "python",
            "request": "launch",
            "module": "test.TestPTA",
           
            "justMyCode": true
        },
        {
            "name": "Test Soundness&Completeness",
            "type": "python",
            "request": "launch",
            "module": "test.TestSoundComplete",
           
            "justMyCode": true
        },
        {
            "name": "Single Test",
            "type": "python",
            "request": "launch",
            "module": "test.SingleTest",
           
            "justMyCode": true
        },
        {
            "name": "Test IR",
            "type": "python",
            "request": "launch",
            "module": "test.TestIR",
            "justMyCode": true
        },
        {
            "name": "Module Finder",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/modulefinder.py",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/test/resources/Import/__main__.py"
            ],
            "justMyCode": true
        }
    ]
}